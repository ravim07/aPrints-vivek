{
  "name": "aprintis-frontend",
  "version": "2.0.0",
  "engines": {
    "node": "12"
  },
  "scripts": {
    "config": "ts-node ./scripts/set-env.ts",
    "ng": "ng",
    "start": "npm run serve:ssr",
    "serve": "ng serve",
    "start:dev": "ng serve --aot",
    "start:no-ssr": "node server.js",
    "build": "npx patch-package && npm run build:ssr",
    "build:app": "npm run config && ng build --prod --aot --vendor-chunk --common-chunk --delete-output-path --buildOptimizer --subresource-integrity=false",
    "build-stats": "npm run config && ng build --prod --aot --vendor-chunk --common-chunk --delete-output-path --buildOptimizer --subresource-integrity=true --stats-json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "stage-build": "git push heroku master",
    "bundle-report": "webpack-bundle-analyzer dist/aprintis-frontend/stats.json",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "compile:polyfill-ie": "webpack --config webpack.polyfill.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "npm run build:app && ng run aprintis-frontend:server:production --bundleDependencies all"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/cdk-experimental": "^8.2.3",
